test_s = "abbccddsmm"

def find_target(child_s):

    times = 0
    flag = True
    for le in child_s:
        if child_s.count(le) % 2 != 0:
            flag = False
#        child_s.replace(le, '')
    return flag

def func(s):
    count = 0
    word_pool = []
    for index, le in enumerate(s):
#        word_pool.append(le)
        for i in range(index, len(s)):
            print s[index:i+1]
            if find_target(s[index:i+1]) == True:
                count += 1

    return count

print func(test_s)
